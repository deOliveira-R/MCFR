FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       90;
    }
    equationOfState
    {
        rho     3320;   // density: kg/m3
    }
    thermodynamics
    {
        Cp      1.5e3;      // specific heat capacity: J/(K.kg) or (kg.m2)/(K.s2.kg)
        Hf      0;
        Sf      0;
    }
    transport
    {
        // mu      50;
        mu      2.5e-2;     // dynamic viscosity: Pa.s or kg/(m.s)
        Pr      1.5e1;      // Prandtl number: (Cp.mu)/k
    }
}

